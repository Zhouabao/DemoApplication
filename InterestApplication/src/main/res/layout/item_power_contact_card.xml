<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusableInTouchMode="true"
    android:overScrollMode="never">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:descendantFocusability="blocksDescendants">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/clInfoBg"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/powerUserBg"
                android:layout_width="0dp"
                android:layout_height="170dp"
                android:layout_marginTop="15dp"
                android:scaleType="fitXY"
                android:src="@drawable/icon_pt_vip_power_user_bg"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <View
                android:id="@+id/powerUserBgExtend"
                android:layout_width="10dp"
                android:layout_height="0dp"
                android:background="@drawable/rectangle_left_ptvip_10dp"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="@id/powerUserBg"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@id/powerUserBg" />

            <TextView
                android:id="@+id/vipOutTime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginRight="15dp"
                android:text="2020年10月12日到期"
                android:textColor="#ffcd7e14"
                android:textSize="12sp"
                app:layout_constraintRight_toRightOf="@id/powerUserBg"
                app:layout_constraintTop_toTopOf="@id/powerUserBg"
                tools:text="2020年10月12日到期" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/vipPowerAvator"
                android:layout_width="72dp"
                android:layout_height="72dp"
                android:layout_marginTop="50dp"
                android:src="@drawable/icon_default_avator"
                app:civ_border_color="#FFCDCDCD"
                app:civ_border_width="1dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@id/powerUserBg" />

            <TextView
                android:id="@+id/vipPowerNickname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:textColor="#ffcd7e14"
                android:textSize="17sp"
                android:textStyle="bold"
                app:layout_constraintLeft_toLeftOf="@id/powerUserBg"
                app:layout_constraintRight_toRightOf="@id/powerUserBg"
                app:layout_constraintTop_toBottomOf="@id/vipPowerAvator"
                tools:text="庾晶梅" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/vipChargeRv"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="12dp"
            android:layout_marginTop="22dp"
            android:layout_marginRight="12dp"
            android:background="@color/transparent"
            android:gravity="center_horizontal"
            android:minHeight="134dp"
            android:orientation="vertical"
            android:overScrollMode="never"
            app:layout_constraintLeft_toLeftOf="@id/clInfoBg"
            app:layout_constraintRight_toRightOf="@id/clInfoBg"
            app:layout_constraintTop_toBottomOf="@id/clInfoBg" />


        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/vipPowerRv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:overScrollMode="never"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/vipChargeRv" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.core.widget.NestedScrollView>

